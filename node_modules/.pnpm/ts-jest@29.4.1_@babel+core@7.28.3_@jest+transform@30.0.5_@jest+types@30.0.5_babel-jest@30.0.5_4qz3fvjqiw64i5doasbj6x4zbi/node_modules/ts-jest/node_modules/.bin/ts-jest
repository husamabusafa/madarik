#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Husam/Dev/madarik/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@30.0.5_4qz3fvjqiw64i5doasbj6x4zbi/node_modules/ts-jest/node_modules:/Users/Husam/Dev/madarik/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@30.0.5_4qz3fvjqiw64i5doasbj6x4zbi/node_modules:/Users/Husam/Dev/madarik/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Husam/Dev/madarik/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@30.0.5_4qz3fvjqiw64i5doasbj6x4zbi/node_modules/ts-jest/node_modules:/Users/Husam/Dev/madarik/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@30.0.5_4qz3fvjqiw64i5doasbj6x4zbi/node_modules:/Users/Husam/Dev/madarik/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
